[run]
branch = True
source = phonon/
include =
    phonon/client.py
    phonon/event.py
    phonon/exceptions.py
    phonon/fields.py
    phonon/lock.py
    phonon/model.py
    phonon/nodelist.py
    phonon/reference.py
    phonon/registry.py
    phonon/__init__.py 
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    def __unicode__
    while True
    time.sleep
precision = 2
show_missing = True
